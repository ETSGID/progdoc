{
    "_comment": "IMPORTANT : go to the wiki page to know about options configuration https://github.com/servocoder/RichFilemanager/wiki/Configuration-options",
    "options": {
        "theme": "default",
        "showTitleAttr": false,
        "showConfirmation": true,
        "browseOnly": true,
        "fileSorting": "NAME_ASC",
        "folderPosition": "bottom",
        "quickSelect": false,
        "logger": false,
        "allowFolderDownload": false,
        "allowChangeExtensions": false,
        "capabilities": [
            "download"
        ]
    },
    "language": {
        "default": "es"
    },
    "formatter": {
        "datetime": {
            "skeleton": "yMMMdHm"
        }
    },
    "filetree": {
        "enabled": false
    },
    "manager": {
        "defaultViewMode": "list",
        "dblClickOpen": false,
        "selection": {
            "enabled": false
        },
        "renderer": {
            "position": false
        }
    },
    "api": {
        "lang": "nodejs",
        "connectorUrl": "/pdi/progdoc/archivos/filemanager",
        "requestParams": {
            "GET": {},
            "POST": {},
            "MIXED": {}
        }
    },

    "security": {
        "readOnly": true,
        "extensions": {
            "policy": "DISALLOW_LIST",
            "ignoreCase": true,
            "restrictions": []
        }
    },
    "upload": {
        "multiple": false,
        "maxNumberOfFiles": 5,
        "chunkSize": false
    },
    "clipboard": {
        "enabled": false
    },
    "filter": {
	"enabled": false
    },
    "search": {
        "enabled": false
    },
    "viewer": {
        "absolutePath": false,
        "previewUrl": false,
        "image": {
            "enabled": true,
            "lazyLoad": true,
            "showThumbs": true,
            "thumbMaxWidth": 64,
            "extensions": [
                "jpg",
                "jpe",
                "jpeg",
                "gif",
                "png",
                "svg"
            ]
        },
        "video": {
            "enabled": false,
            "extensions": [
                "ogv",
                "mp4",
                "webm",
                "m4v"
            ],
            "playerWidth": 400,
            "playerHeight": 222
        },
        "audio": {
            "enabled": false,
            "extensions": [
                "ogg",
                "mp3",
                "wav"
            ]
        },
        "iframe": {
            "enabled": false,
            "extensions": [
                "htm",
                "html"
            ],
            "readerWidth": "95%",
            "readerHeight": "600"
        },
        "opendoc": {
            "enabled": false,
            "extensions": [
                "pdf",
                "odt",
                "odp",
                "ods"
            ],
            "readerWidth": "640",
            "readerHeight": "480"
        },
        "google": {
            "enabled": true,
            "extensions": [
                "doc",
                "docx",
                "xls",
                "xlsx",
                "ppt",
                "pptx"
            ],
            "readerWidth": "640",
            "readerHeight": "480"
        },
        "codeMirrorRenderer": {
            "enabled": false,
            "extensions": [
                "txt",
                "csv"
            ]
        },
        "markdownRenderer": {
            "enabled": true,
            "extensions": [
                "md"
            ]
        }
    },
    "editor": {
        "enabled": false
    },
    "customScrollbar": {
    	"enabled": false
    },
    "extras": {
        "extra_js": [],
        "extra_js_async": true
    },
    "url": "https://github.com/servocoder/RichFilemanager",
    "version": "2.7.6"
}
