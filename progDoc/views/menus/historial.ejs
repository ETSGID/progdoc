

<script src="<%=contextPath%>/js/fileTree/jquery.js" type="text/javascript"></script>
<script src="<%=contextPath%>/js/fileTree/jquery.easing.js" type="text/javascript"></script>
<script src="<%=contextPath%>/js/fileTree/jqueryFileTree.js" type="text/javascript"></script>
<link href="<%=contextPath%>/stylesheets/jqueryFileTree.css" rel="stylesheet" type="text/css" media="screen" />
<div class="contenido3">
    <p>Historial</p>
    <div id='historialDiv'></div>
</div>
<script type="text/javascript">
  $(document).ready(function(){
    $('#historialDiv').fileTree({
      script: 'pdfs',
      root:'/',
      expandSpeed: 1000,
      collapseSpeed: 1000,
      multiFolder: true,
      loadMessage: "Cargando ..."
      }, function(file) {
        if (confirm("Descargar el fichero: " + file.split("/")[file.split("/").length -1])){
          window.open(file);
        }
    });
  });

</script>