<%if(!locals.estado && !locals.permisoDenegado && !locals.existe){%>
    <%switch (session.submenu){
        case "Profesores":%>
            <div>
                <% if(estadoProfesores[departamentoID] === estadosProfesor.abierto && estadoProgDoc === estadosProgDoc.abierto ) { %>
                    <form autocomplete="off" action="<%= aprobarpath %>" method="post" onsubmit="return Enviar()">
                    <input type='submit'  class="btn btn-default" value='Aprobar responsable docente'>
                    </form>
                <% } %>
                    <% if(estadoProfesores[departamentoID] === estadosProfesor.aprobadoResponsable && estadoProgDoc === estadosProgDoc.abierto) { %>
                        <form autocomplete="off" id="formDecision" action="<%= aprobarpath %>" method="post">
                        <input id="decision" type="hidden" name="decision" value='aceptar'>
                        <button type='button'  class="btn btn-primary" onclick="Decidir('aceptar')">Aprobar director departamento</button>
                        <button type="button"  class="btn btn-default" onclick="Decidir('rechazar')">No aprobar</button>
                        </form>
                    <% }
                    if(estadoProfesores[departamentoID] === estadosProfesor.aprobadoDirector && estadoProgDoc === estadosProgDoc.abierto){%>
                        <p>La asignación de profesores ya ha sido aprobada por el director del departamento</p>
                    <%}%>
            </div>
    <%        break;
        case "Profesores2":%>
            <div>
                <button type='button'  class="btn btn-default" onclick="Guardar('<%= nuevopath %>')">Guardar Cambios</button>
                <button type="button"  class="btn btn-default" onclick="Volver('<%= cancelarpath %>')">Cancelar</button>
            </div>
    <%        break;
        case "Tribunales":%>
            <div>
                <%if(estadoTribunales[departamentoID] === estadosTribunal.abierto || estadoProgDoc === estadosProgDoc.incidencia) {
                    if(estadoTribunales[departamentoID] === estadosTribunal.abierto && estadoProgDoc === estadosProgDoc.abierto) {%>
                    <button type='button'  class="btn btn-primary" onclick="Enviar('<%= aprobarpath %>')" >Aprobar responsable docente</button>
                    <%}%>
                    <button type='button'  class="btn btn-default" onclick="Guardar('<%= nuevopath %>')">Guardar Cambios</button>
                    <button type="button"  class="btn btn-default" onclick="Volver('<%= cancelarpath %>')">Cancelar</button>
                <%}else{
                        if(estadoTribunales[departamentoID] === estadosTribunal.aprobadoResponsable && estadoProgDoc === estadosProgDoc.abierto) { %>
                            <form id="formDecision" action="<%= aprobarpath %>" method="post">
                            <input id="decision" type="hidden" name="decision" value='aceptar'>
                            <button type='button'  class="btn btn-primary" onclick="Decidir('aceptar')">Aprobar director departamento</button>
                            <button type="button"  class="btn btn-default" onclick="Decidir('rechazar')">No aprobar</button>
                            </form>
                        <%} if(estadoTribunales[departamentoID] === estadosTribunal.aprobadoDirector && estadoProgDoc === estadosProgDoc.abierto){%>
                            <p>La asignación de tribunales ya ha sido aprobada por el director del departamento</p>
                            <%}
                    }%>
            </div>
    <%        break;
        case "Horarios":%>
            <div>
                <%if(estadoHorarios === estadosHorario.abierto && estadoProgDoc === estadosProgDoc.abierto) {%>
                    <button type='button'  class="btn btn-primary" onclick="Enviar('<%= aprobarpath %>')" >Aprobar coordinador titulación</button>
                <%}%>
                <button type="button"  class="btn btn-default" onclick="Guardar('<%= nuevopath %>')">Guardar Cambios</button>
                <button type="button"  class="btn btn-default" onclick="Volver('<%= cancelarpath %>')">Cancelar</button>
                <button type="button"  class="btn btn-default" onclick="ToggleBotonesRotar()">
                    <svg class="bi bi-pencil" width="1em" height="1em" viewBox="0 0 16 16" fill="currentColor" xmlns="http://www.w3.org/2000/svg">
                        <path fill-rule="evenodd" d="M11.293 1.293a1 1 0 0 1 1.414 0l2 2a1 1 0 0 1 0 1.414l-9 9a1 1 0 0 1-.39.242l-3 1a1 1 0 0 1-1.266-1.265l1-3a1 1 0 0 1 .242-.391l9-9zM12 2l2 2-9 9-3 1 1-3 9-9z"/>
                        <path fill-rule="evenodd" d="M12.146 6.354l-2.5-2.5.708-.708 2.5 2.5-.707.708zM3 10v.5a.5.5 0 0 0 .5.5H4v.5a.5.5 0 0 0 .5.5H5v.5a.5.5 0 0 0 .5.5H6v-1.5a.5.5 0 0 0-.5-.5H5v-.5a.5.5 0 0 0-.5-.5H3z"/>
                    </svg>
                </button>
                <script>
                    function ToggleBotonesRotar() {
                        $( ".btn-rotar-horario" ).toggle();
                    }
                </script>
            </div>
        <%        break;
        case "Actividades":%>
            <div>
                <form id="formulario" role="form" data-toggle="validator" autocomplete="off" action="<%=aprobarpath%>" method="post" onsubmit="return Enviar()" >
                <%if(estadoCalendario === estadosCalendario.abierto && estadoProgDoc === estadosProgDoc.abierto) {%>
                    <input type="submit"  class="btn btn-default" value='Aprobar actividades titulación'>
                <%}%>
                <button type="button"  class="btn btn-default" onclick="anadirConjuntoActividad('conjuntoActividad',false)">Nuevo conjunto de actividades</button>
            </form>
            </div>
    <%        break;
        case "Examenes":%>
            <div>
            <select id="selectExamenes"  onchange="cambiarSelectExamen('<%=selectExamenespath%>')">
                <option value="Examenes" title="Examenes" selected>Exámenes</option>
                <option value="FranjaExamenes" title="FranjaExamenes">Franja Exámenes</option>      
            </select>
                <%if(estadoExamenes === estadosExamen.abierto && estadoProgDoc === estadosProgDoc.abierto) {%>
                    <button type='button'  class="btn btn-primary" onclick="Enviar('<%= aprobarpath %>')" >Aprobar exámenes titulación</button>
                <%}%>
                <button type="button"  class="btn btn-default" onclick="Guardar('<%= nuevopath %>')">Guardar Cambios</button>
                <button type="button"  class="btn btn-default" onclick="Volver('<%= cancelarpath %>')">Cancelar</button>
            </div>
    <%        break;
        case "Examenes2":%>
            <div>
                <select id="selectExamenes"  onchange="cambiarSelectExamen('<%=selectExamenespath%>')">
                <option value="Examenes" title="Examenes">Exámenes</option>
                <option value="FranjaExamenes" title="FranjaExamenes" selected>Franja Exámenes</option>      
                </select>
                    <button type="button"  class="btn btn-default" onclick="Guardar('<%= nuevopath %>')">Guardar Cambios</button>
                    <button type="button"  class="btn btn-default" onclick="Volver('<%= cancelarpath %>')">Cancelar</button>
            </div>
    <%        break;
        default:
            break;
    }
}%>